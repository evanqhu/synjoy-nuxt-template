{
  "name": "synjoy-nuxt-template",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxi dev --dotenv .env.development --host",
    "dev:pre": "nuxi build --dotenv .env.development && nuxi preview",
    "build": "nuxi build --dotenv .env.production",
    "build:dev": "nuxi build --dotenv .env.development",
    "build:stage": "nuxi build --dotenv .env.stage",
    "prepare": "nuxi prepare",
    "preview": "nuxi preview",
    "generate": "nuxi generate",
    "deploy": "PORT=5000 node .output/server/index.mjs",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "dependencies": {
    "@element-plus/icons-vue": ">=0.2.6",
    "@element-plus/nuxt": "1.1.3",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/kit": "^3.17.3",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/tailwindcss": "7.0.0-beta.0",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.5",
    "element-plus": ">=2",
    "firebase": "^11.1.0",
    "marked": "^15.0.8",
    "nuxt": "^3.17.3",
    "pinia": "^3.0.3",
    "vue": "latest",
    "vue-router": "latest",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.4.1",
    "@pinia/nuxt": "^0.11.1",
    "eslint": "^9.29.0",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.6.5",
  "pnpm": {
    "overrides": {
      "ipx": "^3.0.0"
    }
  }
}
